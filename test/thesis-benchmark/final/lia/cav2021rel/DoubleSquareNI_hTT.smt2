(set-logic HORN)

(declare-fun Inv (Int Int Int Int Int Int) Bool)
(declare-fun SchFT (Int Int Int Int Int Int) Bool)
(declare-fun SchTT (Int Int Int Int Int Int) Bool)
(declare-fun SchTF (Int Int Int Int Int Int) Bool)
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int))
  (=> (and (SchFT F D B E C A) (Inv F D B E C A) (<= A 0) (> B 0)) false)
    ))
(assert (forall ((A Int)
         (B Int)
         (C Int)
         (D Int)
         (E Int)
         (F Int)
         (G Int)
         (H Int)
         (I Int)
         (J Int))
       (=> (and (SchTT J H D I F B) (Inv J H D I F B)
                (or (and (<= B 0) (= E F) (= A B))
                    (and (> B 0) (= E (+ F I)) (= A (- B 1))))
                (or (and (<= D 0) (= G H) (= C D))
                    (and (> D 0) (= G (+ H J)) (= C (- D 1))))
                )
           (Inv J G C I E A))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int) (G Int) (H Int))
       (=> (and (SchFT H F C G E B) (Inv H F C G E B)
                (or (and (<= B 0) (= D E) (= A B))
                    (and (> B 0) (= D (+ E G)) (= A (- B 1))))
                    ) (Inv H F C G D A))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int) (G Int) (H Int))
       (=> (and (SchTF H F C G D A) (Inv H F C G D A)
                (or (and (<= C 0) (= E F) (= B C))
                    (and (> C 0) (= E (+ F H)) (= B (- C 1))))) (Inv H E B G D A))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int) (G Int) (H Int))
  (=> (and (Inv H F B G D A) (<= B 0) (<= A 0) (= E F) (= C D) (not (= E C)))
         false)
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int))
  (=> (and (Inv F D B E C A) (or (> B 0) (> A 0)))
      (or (SchTT F D B E C A) (SchFT F D B E C A) (SchTF F D B E C A)))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int))
  (=> (and (= F E) (= D 0) (= C 0) (= B (* 2 F)) (= A (* 2 E)))
         (Inv F D B E C A))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int))
  (=> (and (SchTF F D B E C A) (Inv F D B E C A) (<= B 0) (> A 0)) false)
    ))


(check-sat)
(exit)
