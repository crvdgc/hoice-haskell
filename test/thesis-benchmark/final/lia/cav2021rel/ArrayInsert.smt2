(set-logic HORN)
(declare-fun Inv (Int Int Int Int Int Int Int Int) Bool)
(declare-fun SchTF (Int Int Int Int Int Int Int Int) Bool)
(declare-fun SchTT (Int Int Int Int Int Int Int Int) Bool)

(declare-fun SchFT (Int Int Int Int Int Int Int Int) Bool)
(assert (forall ((A Int)
         (B Int)
         (C Int)
         (D Int)
         (E Int)
         (F Int)
         (G Int)
         (H Int)
         (I Int)
         (J Int)
         (K Int))
       (=> (and (SchTF K C H F I A G D) (Inv K C H F I A G D)
                (or (and (> K 0)
                         (= E F)
                         (= B (+ C 1))
                         (or (>= F C) (= F H))
                         (not (> J 0)))
                    (and (< F C)
                         (> K 0)
                         (> J 0)
                         (= E (+ F 1))
                         (= B C)
                         (not (= F H)))
                    (and (< F C)
                         (= E (+ F 1))
                         (= B C)
                         (not (> K 0))
                         (not (> J 0)))
                    (and (>= F C)
                         (= E F)
                         (= B C)
                         (not (> K 0))
                         (not (> J 0))))
       )
           (Inv J B H E I A G D))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int) (G Int) (H Int))
  (=> (and (Inv H B F D G A E C) (or (< C A) (> G 0) (< D B) (> H 0)))
      (or (SchTT H B F D G A E C) (SchTF H B F D G A E C) (SchFT H B F D G A E C)))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int) (G Int) (H Int))
  (=> (and (> H 0) (> G 0) (= D 0) (= C 0) (= B A))
         (Inv H B F D G A E C))
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int) (G Int) (H Int))
  (=> (and (SchTF H B F D G A E C)
              (Inv H B F D G A E C)
              (>= D B)
              (not (> H 0))
              (or (< C A) (> G 0)))
         false)
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int) (G Int) (H Int))
  (=> (and (SchFT H B F D G A E C)
              (Inv H B F D G A E C)
              (>= C A)
              (not (> G 0))
              (or (< D B) (> H 0)))
         false)
    ))
(assert (forall ((A Int) (B Int) (C Int) (D Int) (E Int) (F Int) (G Int) (H Int))
  (=> (and (Inv H B F D G A E C)
              (>= D B)
              (>= C A)
              (not (> H 0))
              (not (> G 0))
              (not (= D C)))
         false)
    ))
(assert (forall ((A Int)
         (B Int)
         (C Int)
         (D Int)
         (E Int)
         (F Int)
         (G Int)
         (H Int)
         (I Int)
         (J Int)
         (K Int))
       (=> (and (SchFT K C H F J B G E) (Inv K C H F J B G E)
                (or (and (> J 0)
                                  (= D E)
                                  (= A (+ B 1))
                                  (or (>= E B) (= E G))
                                  (not (> I 0)))
                             (and (< E B)
                                  (> J 0)
                                  (> I 0)
                                  (= D (+ E 1))
                                  (= A B)
                                  (not (= E G)))
                             (and (< E B)
                                  (= D (+ E 1))
                                  (= A B)
                                  (not (> J 0))
                                  (not (> I 0)))
                             (and (>= E B)
                                  (= D E)
                                  (= A B)
                                  (not (> J 0))
                                  (not (> I 0)))))
           (Inv K C H F I A G D)
        )
    )
)
(assert (forall ((A Int)
         (B Int)
         (C Int)
         (D Int)
         (E Int)
         (F Int)
         (G Int)
         (H Int)
         (I Int)
         (J Int)
         (K Int)
         (L Int)
         (M Int)
         (N Int))
       (=> (and (SchTT N D J H L B I F) (Inv N D J H L B I F)
                (or (and (> L 0)
                         (= E F)
                         (= A (+ B 1))
                         (or (>= F B) (= F I))
                         (not (> K 0)))
                    (and (< F B)
                         (> L 0)
                         (> K 0)
                         (= E (+ F 1))
                         (= A B)
                         (not (= F I)))
                    (and (< F B)
                         (= E (+ F 1))
                         (= A B)
                         (not (> L 0))
                         (not (> K 0)))
                    (and (>= F B)
                         (= E F)
                         (= A B)
                         (not (> L 0))
                         (not (> K 0))))
                (or (and (> N 0)
                         (= G H)
                         (= C (+ D 1))
                         (or (>= H D) (= H J))
                         (not (> M 0)))
                    (and (< H D)
                         (> N 0)
                         (> M 0)
                         (= G (+ H 1))
                         (= C D)
                         (not (= H J)))
                    (and (< H D)
                         (= G (+ H 1))
                         (= C D)
                         (not (> N 0))
                         (not (> M 0)))
                    (and (>= H D)
                         (= G H)
                         (= C D)
                         (not (> N 0))
                         (not (> M 0))))
        )
        (Inv M C J G K A I E))
    ))


(check-sat)
(exit)
